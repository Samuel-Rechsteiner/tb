@online{modeTEM00,
  title    = {TEM00:Lowest-Order Gaussian Modes Explained},
  url      = {https://ontosight.ai/glossary/term/The-Lowest-Order-Gaussian-Modes---TEM00},
  abstract = {The TEM00 mode, or lowest-order Gaussian mode, is a fundamental concept in optics and laser technology. It is characterized by a Gaussian intensity profile, single peak, and low divergence, making it a stable and desirable mode for various applications. The TEM00 mode is widely used in laser technology, optical fiber communications, and scientific research, including microscopy, interferometry, and quantum optics. Its properties, such as high intensity and low divergence, make it ideal for material processing, spectroscopy, and long-distance optical communication systems. The TEM00 mode's Gaussian profile also matches well with single-mode optical fibers, allowing for efficient data transmission.},
  language = {en},
  urldate  = {2025-07-10},
  journal  = {Ontosight}
}
